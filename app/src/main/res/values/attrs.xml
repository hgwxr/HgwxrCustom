<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="PaoPaoView">
        <attr name="align" >
            <enum name="LEFT" value="0"/>
            <enum name="TOP" value="1"/>
            <enum name="RIGHT" value="2"/>
            <enum name="DOWN" value="3"/>
        </attr>
        <attr name="text"  />
        <attr name="textColor"/>
        <attr name="textSize"/>
        <attr name="backgroudColor"  />
    </declare-styleable>
    <attr name="text" format="string" />
    <attr name="textColor" format="color" />
    <attr name="textSize" format="dimension" />
    <attr name="backgroudColor" format="color" />
    <!--枚举自定义一次，多了报 Error:Error: Found item Attr/more than one time-->
</resources>